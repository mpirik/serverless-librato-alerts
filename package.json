{
  "name": "serverless-librato-alerts",
  "version": "1.0.7",
  "engines": {
    "node": ">= 10"
  },
  "dependencies": {
    "lodash": "~4.17.20",
    "node-fetch": "~2.6.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.161",
    "@types/node-fetch": "2.5.7",
    "@types/serverless": "1.72.6",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "30.3.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.1.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "prettier-plugin-packagejson": "2.2.5",
    "typescript": "4.0.2"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint"
  },
  "lint-staged": {
    "./*.js": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add dist/"
    }
  },
  "repository": "https://github.com/mpirik/serverless-librato-alerts.git"
}
