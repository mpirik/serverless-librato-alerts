{
  "name": "serverless-librato-alerts",
  "version": "1.0.2",
  "engines": {
    "node": ">= 10"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.150",
    "@types/node-fetch": "2.5.6",
    "@types/serverless": "1.67.0",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "23.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.1.0",
    "lint-staged": "10.1.4",
    "prettier": "2.0.4",
    "prettier-plugin-packagejson": "2.2.0",
    "typescript": "3.8.3"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "prepublishOnly": "rm -rf dist && npm run build && npm run lint && npm run test"
  },
  "lint-staged": {
    "./*.js": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add dist/"
    }
  },
  "repository": "https://github.com/mpirik/serverless-librato-alerts.git"
}
